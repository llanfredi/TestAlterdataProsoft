        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore.Hosting.Diagnostics": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "Microsfot.EntityFrameworkCore.Database.Command": "Warning"
